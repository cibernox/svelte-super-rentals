  "version": "0.2.0",
  "configurations": [
      {
          "type": "pwa-node",
          "request": "launch",
          "name": "dev",
          "skipFiles": [
              "<node_internals>/**"
          ],
          "program": "${workspaceFolder}/node_modules/.bin/svelte-kit",
          "args": ["dev"]
      },
      {
          "type": "pwa-node",
          "request": "launch",
          "name": "test",
          "skipFiles": [
              "<node_internals>/**"
          ],
          "program": "${workspaceFolder}/node_modules/.bin/jest",
          "args": ["src"]
      }
  ]
}